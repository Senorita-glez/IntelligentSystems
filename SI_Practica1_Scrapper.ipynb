    "import re\n",
    "import requests\n",
    "from lxml import html\n",
    "\n",
    "#mainPage = 'https://open3dmodel.com/3d-models/'\n",
    "mainPage = 'https://open3dmodel.com/3d-models/page/950'\n",
    "countPages = 950\n",
    "headers = {\n",
    "    'User-Agent': 'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/58.0.3029.110 Safari/537.3'\n",
    "}\n",
    "\n",
    "visitedPages = 950\n",
    "numero_pagina = 0\n",
    "while visitedPages <= countPages:\n",
    "    try:\n",
    "        response = requests.get(mainPage, timeout=6, headers=headers)\n",
    "        tree = html.fromstring(response.content)\n",
    "        \n",
    "        indexes = tree.xpath('/html/body/div[3]/div[3]/div[58]//a[starts-with(@title, \"Page \")]')\n",
    "        lastLink = tree.xpath('/html/body/div[3]/div[3]/div[58]//a')\n",
    "        \n",
    "        if indexes:\n",
    "            patron = r'Page (\\d+)'\n",
    "            maxPage = indexes[-1]\n",
    "            cadena = maxPage.attrib['title'].replace(\",\", \"\")\n",
    "            coincidencias = re.search(patron, cadena)\n",
    "            num_maxPage = int(coincidencias.group(1))\n",
    "            print(cadena, coincidencias)\n",
    "            if num_maxPage > countPages:\n",
    "                countPages = num_maxPage  \n",
    "                              \n",
    "        if lastLink and 'aria-label' in lastLink[-1].attrib and 'rel' in lastLink[-1].attrib and 'class' in lastLink[-1].attrib:\n",
    "            if (\n",
    "                lastLink[-1].attrib['aria-label'] == 'Next'\n",
    "                or lastLink[-1].attrib['rel'] == 'next'\n",
    "                or lastLink[-1].attrib['class'] == 'nextpostslink'\n",
    "            ):\n",
    "                visitedPages += 1\n",
    "                newlink = lastLink[-1].attrib['href']\n",
